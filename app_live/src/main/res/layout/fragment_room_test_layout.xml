<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/rootRootTestContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/light_blue_A400"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/tvRoomTestTips"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="320dp"
            android:gravity="center"
            android:text="New LiveRoom"
            android:textAppearance="@style/BoldTextStyle"
            android:textColor="@color/color_alpha_white_50"
            android:textSize="18dp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvNewTips"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="420dp"
            android:gravity="center"
            android:text="模拟新的直播间类型"
            android:textAppearance="@style/BoldTextStyle"
            android:textColor="@color/white"
            android:textSize="18dp"
            app:layout_constraintTop_toTopOf="parent" />

        <!--如果公用的UI里面还有差异，就建议使用viewStub懒加载创建了。-->
        <ViewStub
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:inflatedId="@layout/fragment_main" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>